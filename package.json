{
  "name": "vite-plugin-ts-to-zod",
  "version": "0.1.3",
  "description": "Vite plugin to transform typescript types to zod schemas",
  "keywords": [
    "vite",
    "vite-plugin",
    "zod"
  ],
  "repository": {
    "url": "https://github.com/gzzhanghao/vite-plugin-ts-to-zod"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "lint-staged": {
    "*.{js,ts,cjs}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@gzzhanghao/ts-to-zod": "^3.16.0"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "1.11.8",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "unbuild": "^3.5.0",
    "vite": "^6.2.1"
  },
  "engines": {
    "pnpm": "^10"
  },
  "publishConfig": {
    "access": "public"
  }
}
